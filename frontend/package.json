{
  "name": "hajimari",
  "version": "0.4.0",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:format": "prettier --config .prettierrc.js --check --cache . --ignore-unknown",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "format": "prettier --config .prettierrc.js --cache . --ignore-unknown --write",
    "preview": "vite preview"
  },
  "dependencies": {
    "@fontsource/roboto": "5.0.13",
    "codemirror": "5.65.16",
    "js-yaml": "4.1.0",
    "svelte-portal": "2.2.1"
  },
  "devDependencies": {
    "@iconify/svelte": "4.0.2",
    "@sveltejs/adapter-static": "3.0.1",
    "@sveltejs/kit": "2.5.9",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/codemirror": "5.60.15",
    "@types/js-yaml": "4.0.9",
    "prettier": "3.2.5",
    "prettier-plugin-packagejson": "2.5.0",
    "prettier-plugin-svelte": "3.2.3",
    "svelte": "4.2.17",
    "svelte-check": "3.7.1",
    "svelte-preprocess": "5.1.4",
    "tslib": "2.6.3",
    "typescript": "5.4.5",
    "vite": "5.2.11"
  }
}
